from tkinter import Variable


# Naming Rules of Variables
# 1name = "kushal" #error
# name1 = "kushal" #error
# name$ = "kushal" #error
# $name = "kushal" #error
# _name = "anyone" #no error

# convention for variable naming
user_one = "kushal" # snake case writing
userOne = "harshit" #came1 case writing

n1 = 2
print (n1) #Variable 1
n1 = 4
print (n1) #Variable 2
n1 = 4
n2 = 8
print (n1 + n2) #Variable 3